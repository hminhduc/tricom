- new_path||=nil
%span#message_confirm_select{hidden: true}=t "app.select_confirm"
%span#message_confirm_delete{hidden: true}=t "app.delete_confirm"
-if new_path
	=link_to t("title.new"),new_path,class: "new-btn dt-button", title: t("title.new")
-else
	%a.new-btn.dt-button{id: "new_#{object}",title: t("title.new")}=t "title.new"
%a.edit-btn.dt-button{id: "edit_#{object}",title: t("title.edit")}=t "title.edit"
%a.delete-btn.dt-button{id: "destroy_#{object}",title: t("title.delete")}=t "title.delete"
